<model>
    <mount>//OPNsense/netsnmp/user</mount>
    <description>Netsnmp user configuration</description>
    <version>1.1</version>
    <items>
        <users>
            <user type="ArrayField">
                <enabled type="BooleanField">
                    <Default>1</Default>
                    <Required>Y</Required>
                </enabled>
                <username type="TextField">
                    <Default></Default>
                    <Required>Y</Required>
                    <Mask>/^([0-9a-zA-Z._\-]){1,64}$/u</Mask>
                    <ValidationMessage>Should be a string between 1 and 64 characters. Allowed characters are 0-9a-zA-Z._-</ValidationMessage>
                </username>
                <password type="TextField">
                    <Default></Default>
                    <Required>N</Required>
                    <Mask>/^([0-9a-zA-Z._\-\!\$\%\/\(\)\+\#\=]){8,64}$/u</Mask>
                    <ValidationMessage>Should be a string between 8 and 64 characters. Allowed characters are 0-9a-zA-Z._-!$%/()+#=</ValidationMessage>
					<Constraints>
                        <check001><type>SetIfConstraint</type><ValidationMessage>Authentication password is required when an authentication protocol is selected.</ValidationMessage><field>authsnmp</field><check>md5</check></check001>
						<check002><type>SetIfConstraint</type><ValidationMessage>Authentication password is required when an authentication protocol is selected.</ValidationMessage><field>authsnmp</field><check>sha</check></check002>
						<check003><type>SetIfConstraint</type><ValidationMessage>Authentication password is required when an authentication protocol is selected.</ValidationMessage><field>authsnmp</field><check>sha-224</check></check003>
						<check004><type>SetIfConstraint</type><ValidationMessage>Authentication password is required when an authentication protocol is selected.</ValidationMessage><field>authsnmp</field><check>sha-256</check></check004>
						<check005><type>SetIfConstraint</type><ValidationMessage>Authentication password is required when an authentication protocol is selected.</ValidationMessage><field>authsnmp</field><check>sha-384</check></check005>
						<check006><type>SetIfConstraint</type><ValidationMessage>Authentication password is required when an authentication protocol is selected.</ValidationMessage><field>authsnmp</field><check>sha-512</check></check006>
					</Constraints>
                </password>
                <enckey type="TextField">
                    <Default></Default>
                    <Required>N</Required>
                    <Mask>/^([0-9a-zA-Z._\-\!\$\%\/\(\)\+\#\=]){8,64}$/u</Mask>
                    <ValidationMessage>Should be a string between 8 and 64 characters. Allowed characters are 0-9a-zA-Z._-!$%/()+#=</ValidationMessage>
					<Constraints>
                        <check001><type>SetIfConstraint</type><ValidationMessage>Privacy password is required when an privacy protocol is selected.</ValidationMessage><field>privacysnmp</field><check>des</check></check001>
						<check002><type>SetIfConstraint</type><ValidationMessage>Privacy password is required when an privacy protocol is selected.</ValidationMessage><field>privacysnmp</field><check>aes</check></check002>
						<check003><type>SetIfConstraint</type><ValidationMessage>Privacy password is required when an privacy protocol is selected.</ValidationMessage><field>privacysnmp</field><check>aes-192</check></check003>
						<check004><type>SetIfConstraint</type><ValidationMessage>Privacy password is required when an privacy protocol is selected.</ValidationMessage><field>privacysnmp</field><check>aes-256</check></check004>
					</Constraints>
                </enckey>
                <authsnmp type="OptionField">
					<Default>none</Default>
					<Required>Y</Required>
                    <OptionValues>
                        <none>Disabled</none>
                        <md5>MD5</md5>
                        <sha>SHA</sha>
                        <sha-224>SHA-224</sha-224>
                        <sha-256>SHA-256</sha-256>
                        <sha-384>SHA-384</sha-384>
                        <sha-512>SHA-512</sha-512>
                    </OptionValues>
                </authsnmp>
                <privacysnmp type="OptionField">
					<Default>none</Default>
					<Required>Y</Required>
                    <OptionValues>
                        <none>Disabled</none>
                        <des>DES</des>
                        <aes>AES</aes>
                        <aes-192>AES-192</aes-192>
                        <aes-256>AES-256</aes-256>
					</OptionValues>
                    <Constraints>
                        <check001>
                            <ValidationMessage>You cannot set the privacy protocol without setting the authentication protocol.</ValidationMessage>
                            <type>DependConstraint</type>
                            <addFields>
                                <field1>password</field1>
                            </addFields>
                        </check001>
                    </Constraints>
                </privacysnmp>
                <readwrite type="BooleanField">
                    <Default>0</Default>
                    <Required>Y</Required>
                </readwrite>
            </user>
        </users>
    </items>
</model>
